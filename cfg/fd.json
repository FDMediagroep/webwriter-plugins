{
    "plugins": [
      {
        "vendor": "infomaker.se",
        "name": "ximteaser",
        "enabled": true,
        "data": {
          "fields": [
            "subject",
            "text"
          ]
        }
      },
      {
        "vendor": "infomaker.se",
        "name": "publish",
        "enabled": true
      },
      {
        "vendor": "infomaker.se",
        "name": "youtube",
        "enabled": true,
        "data": {
          "urlMatchers": [
            "^https?://youtu\\.be/.*",
            "^https?://(www\\.)?youtube\\.com/watch\\?v=.*"
          ]
        }
      },
      {
        "vendor": "infomaker.se",
        "name": "preamble",
        "enabled": true
      },
      {
        "vendor": "infomaker.se",
        "name": "utils",
        "enabled": true
      },
      {
        "vendor": "nl.fdmg",
        "name": "oembed",
        "enabled": true,
        "data": {
          "urlMatchers": [
            "^http(?:s)?://twitter\\.com/(?:#!)?[^#?/]+/status/.+$",

            "^https?://(www\\.)?instagram\\.com/p/.*",
            "^https?://(www\\.)?instagr\\.am/p/.*",

            "^https?://(www\\.)?soundcloud\\.com/.*/.*",

            "^https?://(www\\.)?vimeo\\.com/.*",
            "^https?://player\\.vimeo.com/video/.*",

            "^https://www\\.facebook\\.com/.*/posts/.*",
            "^https://www\\.facebook\\.com/.*/activity/.*",
            "^https://www\\.facebook\\.com/photo\\.php?fbid=.*",
            "^https://www\\.facebook\\.com/photos/.*",
            "^https://www\\.facebook\\.com/permalink\\.php\\?story_fbid=.*",
            "^https://www\\.facebook\\.com/media/set\\?set=.*",
            "^https://www\\.facebook\\.com/questions/.*",
            "^https://www\\.facebook\\.com/notes/.*/.*/.*",

            "^https://www\\.facebook\\.com/.*/videos/.*/",
            "^https://www\\.facebook\\.com/video\\.php\\?id=.*",
            "^https://www\\.facebook\\.com/video\\.php\\?v=.*"
          ]
        }
      },
      {
         "vendor": "infomaker.se",
         "name": "ximimage",
         "enabled": true,
         "data": {
             "enablesoftcrop": true,
             "publishedmaxwidth": 2560,
             "crops": {
               "16:9": [16, 9],
               "4:3": [4, 3],
               "1:1": [1, 1]
           },
           "urlMatchers": [
             "^.*\\.jpg",
             "^.*\\.gif",
             "^.*\\.png"
           ]
         }
       },
      {
        "vendor": "infomaker.se",
        "name": "author",
        "enabled": false
      },
      {
        "vendor": "infomaker.se",
        "name": "tags",
        "enabled": false
      },
      {
        "vendor": "nl.fdmg",
        "name": "validatable",
        "enabled": true
      },
      {
        "vendor": "nl.fdmg",
        "name": "linkselector",
        "enabled": true
      },
      {
        "vendor": "nl.fdmg",
        "name": "articleoptions",
        "enabled": true
      },
      {
        "vendor": "nl.fdmg",
        "name": "byline",
        "enabled": false,
        "data": {
         "backendPath": "https://dev-api.fdmg.org/private/fd/articles/authors"
        }
      },
      {
        "vendor": "nl.fdmg",
        "name": "defaultvalidation",
        "enabled": false,
        "data" : {
          "mandatory" : [
            {"type" : "fdmg/section", "name" : "section", "message" : "Geen sectie opgegeven"}
          ]
